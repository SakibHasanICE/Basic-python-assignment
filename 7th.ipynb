   "source": [
    "lower = 10\n",
    "upper = 1000\n",
    "sum_primes = 0\n",
    "\n",
    "for num in range(lower, upper + 1):\n",
    "    if num > 1:\n",
    "        for i in range(2, num):\n",
    "            if (num % i) == 0:\n",
    "                break\n",
    "        else:\n",
    "            sum_primes += num\n",
    "\n",
    "print(\"Sum of prime numbers:\", sum_primes)"
   ]
  }
